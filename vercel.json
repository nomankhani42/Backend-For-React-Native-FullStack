{
  "version": 2,
  "functions": {
    "api/**.js": {
      "maxDuration": 60 
    }
  },
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
